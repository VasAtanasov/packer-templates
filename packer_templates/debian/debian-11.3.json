{
  "builders": [
    {
      "type": "null",
      "communicator": "none"
    }
  ],
  "post-processors": [
    [
      {
        "type": "artifice",
        "files": [
          "{{ user `build_directory` }}/{{user `box_basename`}}.{{.Provider}}.box"
        ]
      },
      {
        "type": "vagrant-cloud",
        "box_tag": "vasatanasov/debian-11.3",
        "version": "{{user `version`}}",
        "version_description": "Debian 11.3 Vagrant box. Built with: virtualbox: 6.1.32, packer: 1.8.2."
      }
    ]
  ],
  "variables": {
    "box_basename": "debian-11.3",
    "build_directory": "../../builds",
    "version": ""
  }
}